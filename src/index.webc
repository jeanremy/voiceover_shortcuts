---
layout:layout.webc
---
<div class="container">
  <h1>
    VoiceOver shortcuts
  </h1>

  <form action="">
    <label for="search">Search</label>
    <input id="search" type="text">
    <input type="submit" value="Submit">
  </form>

  <main>
    <ul class="results">
      <shortcut webc:for="shortcut of shortcuts" :@label="shortcut.label" :@keys="shortcut.shortcut" webc:nokeep />
    </ul>
  </main>
</div>

<link rel="stylesheet" href="assets/css/index.css" webc:keep>